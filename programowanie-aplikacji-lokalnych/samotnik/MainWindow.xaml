<Window x:Class="samotnik.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:samotnik"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="500">
    <Grid x:Name="MainGrid">
        <Button x:Name="BackButton" Content="Cofnij" HorizontalAlignment="Right" Margin="0,5,5,0" VerticalAlignment="Top" Height="40" Width="100" Click="BackButton_Click" IsEnabled="False"/>
        <Button x:Name="ClearButton" Content="Wyczyść" HorizontalAlignment="Right" Margin="0,50,5,0" VerticalAlignment="Top" Height="40" Width="100" Click="ClearButton_Click"/>
    </Grid>
</Window>
